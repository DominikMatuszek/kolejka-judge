{
  "args": [
    "python3",
    "judge.py",
    "--observer",
    "--tests",
    "tests/tests.yaml",
    "--solution",
    "solution/hello.cpp",
    "--output-directory",
    "results",
    "--results",
    "results.yaml"
  ],
  "collect": [
    {
      "glob": "results/results.yaml"
    },
    {
      "glob": "results/1/log.zip"
    },
    {
      "glob": "results/1/satori/**"
    }
  ],
  "environment": {},
  "exclusive": false,
  "files": {
    "KolejkaJudge.zip": {
      "path": "KolejkaJudge.zip"
    },
    "judge.py": {
      "path": "judge.py"
    },
    "solution/hello.cpp": {
      "path": "solution/hello.cpp"
    },
    "tests/001/1.in": {
      "path": "tests/001/1.in"
    },
    "tests/002/1.out": {
      "path": "tests/002/1.out"
    },
    "tests/tests.yaml": {
      "path": "tests/tests.yaml"
    }
  },
  "image": "kolejka/satori:judge",
  "limits": {
    "cpus": 1,
    "memory": "4294967296b",
    "network": false,
    "pids": 256,
    "storage": "2147483648b",
    "swap": "0b",
    "time": "21.0s",
    "workspace": "2147483648b"
  },
  "requires": [
    "cpu:xeon e3-1270 v5"
  ],
  "stderr": "console_stderr.txt",
  "stdout": "console_stdout.txt"
}